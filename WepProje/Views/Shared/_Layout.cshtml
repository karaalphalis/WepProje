<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - HD Film</title>

    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css">

    <link href="~/Content/owl.carousel.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/owl.theme.default.min.css" rel="stylesheet" type="text/css" />

    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Scripts/owl.carousel.min.js"></script>

    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/PagedList.css" rel="stylesheet" />



</head>

<body>
    <nav class="navbar navbar-expand-xl navbar-light bg-light">

        <div class="nav-item mt-2 ml-xl-5 ml-sm-3">
            <a href="/Search/Index"><i class="fa fa-search"></i></a>
        </div>

        <a href="/" class="navbar-brand ml-xl-5 ">
            <i class="fas fa-blog"></i>&nbsp;Blog Template
        </a>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls=“navbarNav” aria-label=“Toggle navigation”>
            <span class="navbar-toggler-icon "></span>

        </button>
        <div id="navbarCollapse" class="collapse navbar-collapse">
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a href="/CategoriyBlog/Index" class="nav-link">Kategoriler</a>
                </li>


            </ul>


        </div>

    </nav>
    <div class="container">
        <div class="row ">
            <div class="col-md-12 ">
                @RenderBody()
            </div>
        </div>

        <hr />


        <footer>
            @Html.Action("Index", "Coursel")
        </footer>


    </div>

    <script src="~/Scripts/ap.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/umd/popper.min.js"></script>





</body>
</html>